//transformation of the arrray  
//map method
const arr=[5,1,2,6];
function double(x){
    return x * 2;
}
function triple(x){
    return x * 3;
}
function square(x){
    return x * x;
}

function binary(x){
    return x.toString(2);

}
const output=arr.map(function double(x){
    return x * 2;
});
const output1=arr.map((x)=> x * 3);

const output2=arr.map(square);
const output3=arr.map(binary);


console.log(output);
console.log(output1);
console.log(output2);
console.log(output3);